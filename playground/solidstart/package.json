    "autoprefixer": "^10.4.13",
    "eslint": "^8.34.0",
    "eslint-plugin-solid": "^0.9.4",
    "postcss": "^8.4.21",
    "solid-start-node": "^0.2.26",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "vite": "^4.1.2"
